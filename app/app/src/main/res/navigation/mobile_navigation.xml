<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/mobile_navigation"
	app:startDestination="@+id/navigation_home">

	<fragment
		android:id="@+id/navigation_home"
		android:name="space.metamap.ui.feed.FeedFragment"
		android:label="@string/title_feed"
		tools:layout="@layout/fragment_feed" />

	<fragment
		android:id="@+id/navigation_dashboard"
		android:name="space.metamap.ui.map.MapFragment"
		android:label="@string/title_map"
		tools:layout="@layout/fragment_map" />

	<fragment
		android:id="@+id/navigation_notifications"
		android:name="space.metamap.ui.message.MessageFragment"
		android:label="@string/title_message"
		tools:layout="@layout/fragment_message" />
</navigation>
